{
  "name": "proto2diagram",
  "version": "1.0.0",
  "description": "Convert Protocol Buffer definitions to PlantUML diagrams",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "example",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "protobuf",
    "protocol-buffers",
    "plantuml",
    "diagram",
    "visualization",
    "uml",
    "converter",
    "library",
    "javascript",
    "plantuml generator",
    "proto visualizer"
  ],
  "author": "Room-Elephant",
  "license": "MIT",
  "homepage": "https://github.com/Room-Elephant/proto2diagram#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Room-Elephant/proto2diagram.git",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/Room-Elephant/proto2diagram/issues"
  },
  "scripts": {
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "example": "node ./example/example.js"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "protobufjs": "^7.5.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "5.2.2"
  }
}
